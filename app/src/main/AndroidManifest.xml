<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.indoorlocation.SmartStep">

    <application
        android:name="com.example.SmartStep.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.SmartStep.OpenLoaction"></activity>
        <activity android:name="com.example.SmartStep.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.SmartStep.AccelerometerData">

        </activity>
        <activity android:name="com.example.SmartStep.MapActivity"></activity>
    </application>

</manifest>